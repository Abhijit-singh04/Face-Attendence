                    <script>
                        $(document).ready(function () {
                            $('#studentTable').empty();

                            $('#getInfoBtn').click(function () {

                                var selectGroup = $('#selectGroup').val();
                                var class_date = $('#class_date').val();
                                console.log("class date ");
                                console.log(class_date);

                                $.ajax({
                                    type: 'GET',
                                    data: {
                                        selectGroup: selectGroup,
                                        class_date: class_date
                                    },
                                    // data:{_SMRobj:JSON.stringify(obj)},
                                    url: '/getData',
                                    success: function (data) {
                                        console.log(data.data);
                                        var tableData = data.data;
                                        $('#studentID').empty();
                                        $('#studentName').empty();
                                        $('#studentTable').append(
                                            " <thead><tr><td>MISIS</td><td>Name</td><td>Status</td></tr></thead>"
                                        );
                                        for (i = 0; i < tableData.length; i++) {
                                            var row = tableData[i];
                                            $('#studentTable').append('<tbody><tr><td>' +
                                                row.MISIS + '</td><td>' + row
                                                .student_firstName + '</td><td>' + row
                                                .status + '</td><td></tbody>')
                                        }
